   "source": [
    "# ex 2 - primeira integral\n",
    "import sympy as sp\n",
    "\n",
    "x = sp.Symbol('x')\n",
    "x0 = sp.Symbol('x0')\n",
    "e = sp.Symbol('e')\n",
    "g = sp.Symbol('g')\n",
    "x1, x2 = x0-e,x0+e\n",
    "f1 = ((x-x1)/e)**2\n",
    "f2 = ((x2-x)/e)**2\n",
    "int1 = sp.integrate(f1, (x,x1,x0))\n",
    "int2 = sp.integrate(f2, (x,x0,x2))\n",
    "res = sp.simplify(int1+int2)\n",
    "res\n"
   ]
  }
